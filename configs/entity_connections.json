{
  "connections_version": "2.0.0",
  "architecture_compliance": "navigation_optimized",
  "metadata": {
    "last_updated": "2025-06-02",
    "cache_ttl_seconds": 1200,
    "navigation_integration": true,
    "routing_optimization": true
  },
  "supporting_connections": {
    "Facility": "ActionRequest",
    "Department": "ActionRequest",
    "Asset": "Problem",
    "RecurringStatus": "Problem",
    "AmountOfLoss": "Problem",
    "Review": "ActionPlan",
    "EquipmentStrategy": "ActionPlan"
  },
  "connection_display": {
    "show_arrows": true,
    "connection_symbol": "â†’",
    "connection_style": "text-white-50",
    "visual_optimization": {
      "arrow_animation": {
        "enabled": false,
        "duration": "2s",
        "direction": "forward"
      },
      "connection_highlighting": {
        "on_hover": true,
        "highlight_color": "#4A90E2",
        "highlight_width": "2px"
      },
      "responsive_visibility": {
        "hide_on_mobile": true,
        "simplify_on_tablet": true,
        "full_display_desktop": true
      }
    }
  },
  "navigation_routing": {
    "entity_navigation_config": {
      "Facility": {
        "route_pattern": "/facility/{facility_id}",
        "validation_required": true,
        "adapter_validation": "facility_adapter.validate_facility_data",
        "breadcrumb_label": "Facility: {facility_name}",
        "navigation_context": "facility_detail"
      },
      "ActionRequest": {
        "route_pattern": "/action-request/{action_request_id}",
        "validation_required": false,
        "parent_navigation": "/facility/{facility_id}",
        "breadcrumb_label": "Action Request: {title}",
        "navigation_context": "workflow_detail"
      },
      "Problem": {
        "route_pattern": "/problem/{problem_id}",
        "validation_required": false,
        "parent_navigation": "/action-request/{action_request_id}",
        "breadcrumb_label": "Problem Analysis",
        "navigation_context": "workflow_detail"
      },
      "RootCause": {
        "route_pattern": "/root-cause/{root_cause_id}",
        "validation_required": false,
        "parent_navigation": "/problem/{problem_id}",
        "breadcrumb_label": "Root Cause Analysis",
        "navigation_context": "causal_analysis"
      }
    },
    "route_optimization": {
      "url_caching": {
        "enabled": true,
        "cache_ttl_seconds": 900,
        "validation_cache": true
      },
      "deep_linking": {
        "enabled": true,
        "state_preservation": true,
        "context_restoration": true
      },
      "navigation_performance": {
        "preload_related_routes": true,
        "route_prefetching": false,
        "lazy_route_loading": true
      }
    }
  },
  "workflow_connections": {
    "primary_workflow_flow": [
      {
        "from": "ActionRequest",
