        "relationship": "IDENTIFIED_IN",
        "navigation_enabled": true,
        "bi_directional": true
      },
      {
        "from": "Problem",
        "to": "RootCause",
        "relationship": "ANALYZES",
        "navigation_enabled": true,
        "bi_directional": true
      },
      {
        "from": "RootCause",
        "to": "ActionPlan",
        "relationship": "RESOLVES",
        "navigation_enabled": true,
        "bi_directional": true
      },
      {
        "from": "ActionPlan",
