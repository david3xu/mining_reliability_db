  "adapter_optimization": {
    "facility_adapter": {
      "description": "Handles facility reference data and location mappings",
      "primary_entities": ["Facility"],
      "cache_strategy": "persistent",
      "access_patterns": ["reference_data", "location_lookup"]
    },
    "data_adapter": {
      "description": "Main data access for ActionRequest and Department entities",
      "primary_entities": ["ActionRequest", "Department"],
      "optimization_level": "high_frequency",
      "indexing_strategy": "composite"
    },
    "workflow_adapter": {
      "description": "Sequential workflow processing for Problem, RootCause, ActionPlan, Verification, Review",
      "primary_entities": [
        "Problem",
        "RootCause",
        "ActionPlan",
        "Verification",
        "Review"
      ],
      "stage_aware": true,
      "dependency_tracking": true
    },
    "asset_adapter": {
      "description": "Asset management and maintenance tracking",
      "primary_entities": ["Asset"],
      "integration_points": ["equipment_registry", "maintenance_schedule"]
    },
    "intelligence_adapter": {
      "description": "Pattern analysis and recurring issue detection",
      "primary_entities": ["RecurringStatus"],
      "analytics_enabled": true,
      "machine_learning_ready": true
    },
    "financial_adapter": {
      "description": "Financial impact and loss quantification",
      "primary_entities": ["AmountOfLoss"],
      "calculation_engine": "enabled",
      "reporting_integration": true
    },
    "strategy_adapter": {
      "description": "Equipment strategy and document management",
      "primary_entities": ["EquipmentStrategy"],
      "document_integration": "APSS",
      "strategy_tracking": true
    }
